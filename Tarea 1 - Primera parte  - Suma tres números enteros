public class Main {

    public static void main(String[] args) {
        int param1 = 10;
        int param2 = 20;
        int param3 = 30;

        int suma = suma(param1,param2,param3);

        System.out.println("La suma es = " + suma);
    }

    // Funcion que suma tres valores.
    public static int suma (int a, int b, int c) {

        return a+b+c;
    }
}
